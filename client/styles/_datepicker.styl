  position fixed
  top 0
  left 0
  height 100%
  width 100% }

.SingleDatePicker__picker--full-screen-portal {
  background-color #fff }

.SingleDatePicker__close {
  background none
  border 0
  color inherit
  font inherit
  line-height normal
  overflow visible
  padding 0
  cursor pointer
  position absolute
  top 0
  right 0
  padding 15px
